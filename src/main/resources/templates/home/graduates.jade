doctype html
head
	meta(charset="utf-8")
	meta(http-equiv="X-UA-Compatible", content="IE=edge")
	title Истории выпускников
	link(rel='stylesheet', href='/css/graduate.css', type='text/css', media='screen')
	include general-includes.jade
body
	include forms/header.jade
	include forms/lost.jade
	include forms/enrollee-mixin.jade
	if (root)
		include forms/redact-content.jade
		+redact-content(canCreate, createType, canRedact, canDelete, redactId, deleteId)
	div.container.graduate-main-container
		each tmp in content
			+enrollee-page-add-reviews(tmp);